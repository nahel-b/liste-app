{
  "expo": {
    "name": "phlow",
    "slug": "phlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.nahelb.phlow"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOGO_APP.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nahelb.listeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bf15b207-818e-471a-a0ad-b614ace8f99a"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
          "expo-build-properties",
          {
            "ios": {
              "useFrameworks": "static"
            }
          }
        ]
    ]
    
  }
}
